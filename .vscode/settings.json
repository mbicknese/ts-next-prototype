{
    "typescript.implementationsCodeLens.enabled": true,
    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      {"language": "typescript", "autoFix": true },
      {"language": "typescriptreact", "autoFix": true }
    ],
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/coverage": true,
      "**/.DS_Store": true,
      "**/.next": true,
      "**/.vscode": true,
      "**/node_modules": true,
      "**/package-lock.json": true
    }
}
